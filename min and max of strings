#include<iostream>
int main()
{
	int a[20],i,j,n;
	printf("enter the no.of elements");
	scanf("%d",&n);
	printf("enter the elements\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	int max=a[0];
	int min=a[0];
	for(i=0;i<n;i++)
	{
		if(max<a[i])
		{
			max=a[i];
		}
		else if(min>a[i])
		{
			min=a[i];
		}
		else
		{
			continue;
		}
	}
	printf("max=%d\n",max);
	printf("min=%d\n",min);
	
}
