#include<stdio.h>
#include<conio.h>
int main()
{
    int arr[3], i, num, pos;
    printf("Enter any 3 Numbers: ");
    for(i=0; i<3; i++)
        scanf("%d", &arr[i]);
    printf("\nEnter a Number to Search: ");
    scanf("%d", &num);
    for(i=0; i<3; i++)
    {
        if(arr[i]==num)
        {
            pos=i+1;
            break;
        }
    }
    printf("\nFound at Index No.%d", pos);
    getch();
    return 0;
}
